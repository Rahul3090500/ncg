{
  "functions": {
    "src/app/(payload)/admin/[[...segments]]/page.tsx": {
      "maxDuration": 60
    },
    "src/app/(payload)/api/[[...slug]]/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*"
    }
  ]
}